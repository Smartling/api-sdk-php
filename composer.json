{
  "name": "smartling/api-sdk-php",
  "version": "5.0.1",
  "type": "library",
  "description": "PHP library to communicate with SmartlingAPI",
  "keywords": [
    "smartling",
    "i18n",
    "internationalization",
    "automated translation"
  ],
  "homepage": "https://www.smartling.com",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "psr/log": "^1.0.0 || ^3.0.0",
    "php": "^7.3 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.0.0 || ^7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8"
  },
  "autoload": {
    "psr-4": {
      "Smartling\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smartling\\Tests\\Unit\\": "tests/unit",
      "Smartling\\Tests\\Functional\\": "tests/functional"
    }
  },
  "minimum-stability": "dev"
}
